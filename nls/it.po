msgstr "Uso: %s [ opzioni ] [ <intervallo> [ <iterazioni> ] ]\n"

#: cifsiostat.c:72 nfsiostat.c:71
#, c-format
msgid ""
"Options are:\n"
"[ -h ] [ -k | -m ] [ -t ] [ -V ]\n"
msgstr ""
"Le opzioni sono:\n"
"[ -h ] [ -k | -m ] [ -t ] [ -V ]\n"

#: common.c:57
#, c-format
msgid "sysstat version %s\n"
