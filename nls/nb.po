"[ -c ] [ -d ] [ -h ] [ -k | -m ] [ -N ] [ -t ] [ -V ] [ -x ] [ -z ]\n"
"[ -j { ID | LABEL | PATH | UUID | ... } ]\n"
"[ [ -T ] -g <gruppenavn> ] [ -p [ <enhet> [,...] | ALL ] ]\n"
"[ <enhet> [...] | ALL ] [ --debuginfo ]\n"

