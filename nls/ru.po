"[ -U [ <имя_пользователя> ] ] [ -u ] [ -V ] [ -v ] [ -w ] [ -C <команда> ]\n"
"[ -G <имя_процесса> ] [ -p { <pid> [,…] | SELF | ALL } ]\n"
